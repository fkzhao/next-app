{
  "build": {
    "env": {
      "GO_BUILD_FLAGS": "-ldflags '-s -w'"
    }
  },
  "rewrites": [
    {
      "source": "/app/:params*",
      "destination": "/api/proxy",
      "has": [
        {
          "type": "header",
          "key": "x-vercel-token",
          "value": "buse_interstitial=vocal-gibbon-genuinely.ngrok-free.app"
        }
      ]
    },
    {
      "source": "/app",
      "destination": "/api/v1/index",
      "has": [
        {
          "type": "header",
          "key": "x-vercel-token",
          "value": "buse_interstitial=vocal-gibbon-genuinely.ngrok-free.app"
        }
      ]
    }
  ]
}